{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "docfetch-compile",
      "type": "shell",
      "command": "npm run compile",
      "options": {
        "cwd": "${workspaceFolder}/extensions/docfetch"
      },
      "problemMatcher": "$tsc",
      "group": "build"
    },
    {
      "label": "docfetch-watch",
      "type": "shell",
      "command": "npm run watch",
      "options": {
        "cwd": "${workspaceFolder}/extensions/docfetch"
      },
      "isBackground": true,
      "problemMatcher": "$tsc-watch",
      "group": "build"
    }
  ]
}
