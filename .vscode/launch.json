{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run DocFetch Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/extensions/docfetch"],
      "outFiles": ["${workspaceFolder}/extensions/docfetch/out/**/*.js"],
      "preLaunchTask": "docfetch-compile"
    }
  ]
}
